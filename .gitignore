bower_components/

# Compiled output
build/
dist/

# Optional npm cache directory
.npm/

# Optional eslint cache
.eslintcache

# dotenv environment variables file
# .env
.env.*

# Mac system files
.DS_Store

# Windows system files
Thumbs.db

# VSCode settings
.vscode/

# IDEs and editors
.idea/
*.suo
*.ntvs*
*.njsproj
*.sln
*.sw?

# TypeScript cache
*.tsbuildinfo

# PM2 logs
pmlogs/

# Misc
*.tgz

# Local .env files
.env.local
.env.development.local
.env.test.local
.env.production.local

config.yml
